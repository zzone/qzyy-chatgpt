{
  "name": "qzyy-chatgpt-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "chat": "vite serve src/apps/chat/ --config ./vite.config.ts",
    "admin": "vite serve src/apps/admin/ --config ./vite.config.ts",
    "build": "vue-tsc && vite build",
    "build:chat": "vue-tsc --noEmit && vite build --mode chat",
    "build:admin": "vue-tsc --noEmit && vite build --mode admin",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .js,.vue src"
  },
  "dependencies": {
    "@types/node": "^18.15.10",
    "eslint": "^8.36.0",
    "less": "^4.1.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@esbuild/win32-x64": "^0.17.14",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "prettier": "^2.8.7",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.2.0"
  }
}
